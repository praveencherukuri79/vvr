{
  "name": "vvr",
  "version": "0.0.0",
  "scripts": {
    "prettier-format": "prettier --config .prettierrc \"src/**/*.{ts,css,scss,js,html}\" --write",
    "prettier-format:staged": "prettier --config .prettierrc --write",
    "create-env": "printenv > .env",
    "ng": "ng",
    "start": "node dist/vvr/server/main.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run vvr:serve-ssr",
    "serve:ssr": "node dist/vvr/server/main.js",
    "build:ssr": "ng build && ng run vvr:server",
    "build:ssr:dev": "ng build && ng run vvr:server:development",
    "build:ssr:stats": "ng build --stats-json && ng run vvr:server --stats-json",
    "webpack-analyzer:browser": "webpack-bundle-analyzer dist/vvr/browser/stats.json",
    "webpack-analyzer:server": "webpack-bundle-analyzer dist/vvr/server/stats.json",
    "prerender": "ng run vvr:prerender",
    "start:server:dev": "cross-env NODE_ENV=development node -r dotenv/config dist/vvr/server/main.js dotenv_config_path=.env",
    "start:server:dev2": "cross-env NODE_ENV=production node -r dotenv/config dist/vvr/server/main.js dotenv_config_path=.env",
    "start:server:prod": "cross-env NODE_ENV=production node dist/vvr/server/main.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.0.3",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.0.3",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/platform-server": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@nguniversal/express-engine": "^15.0.0",
    "express": "^4.15.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "xlsx": "0.18.5",
    "zone.js": "~0.12.0",
    "mongodb": "4.13.0",
    "mongoose": "6.8.1",
    "jsonwebtoken": "9.0.0",
    "bcryptjs": "2.4.3",
    "cron": "2.1.0",
    "dotenv": "16.0.3",
    "axios": "1.2.1",
    "request-promise": "4.2.6",
    "google-auth-library":"8.7.0",
    "nodemailer": "6.8.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.4",
    "@angular/cli": "~15.0.4",
    "@angular/compiler-cli": "^15.0.0",
    "@nguniversal/builders": "^15.0.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^14.15.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.2",
    "concurrently": "7.6.0",
    "@types/jsonwebtoken": "9.0.0",
    "@types/request-promise": "4.1.48",
    "@adamjlev/prettier": "2.1.0",
    "lint-staged": "^10.5.3",
    "husky": "^4.3.6",
    "cross-env": "7.0.3",
    "@types/nodemailer": "6.4.7",
    "webpack-bundle-analyzer": "4.7.0"
  }
}
